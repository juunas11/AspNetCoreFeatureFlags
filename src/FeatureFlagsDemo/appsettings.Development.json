{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FeatureManagement": {
    "FeatureFlags": {
      "NewsSummary": false,
      "DarkTheme": {
        "EnabledFor": [
          {
            "Name": "AppVersion",
            "Parameters": {
              "Versions": {
                "Stable": "OptIn",
                "Beta": "Enabled"
              }
            }
          }
        ]
      },
      "UserGreeting": {
        "EnabledFor": [
          {
            "Name": "UserPercentage",
            "Parameters": {
              "Percentage": 30
            }
          }
        ]
      }
    }
  }
}
